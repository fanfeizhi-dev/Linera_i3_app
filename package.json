    "playwright": "^1.56.1",
    "vite": "^7.1.3"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.30.1",
    "@openzeppelin/contracts": "^5.4.0",
    "@solana/web3.js": "^1.98.4",
    "cors": "^2.8.5",
    "express": "^4.18.0",
    "firebase": "^12.2.1",
    "node-fetch": "^2.7.0"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
