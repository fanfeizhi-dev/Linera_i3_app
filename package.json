{
  "name": "intelligence-cubed-homepage",
  "version": "1.0.0",
  "description": "Intelligence Cubed homepage with ChainOpera-style interface",
  "main": "server.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build && node scripts/copy-static.js",
    "preview": "vite preview --port 3000",
    "start": "node serve.js",
    "start:3000": "node serve.js",
    "gen:emb": "node scripts/gen-model-embeddings.js",
