    "firebase:deploy": "npm run build && firebase deploy --only hosting",
    "firebase:serve": "npm run build && firebase serve --only hosting"
  },
  "keywords": [
    "intelligence-cubed",
    "ai",
    "terminal",
    "homepage"
  ],
  "author": "Intelligence Cubed Team",
  "license": "MIT",
